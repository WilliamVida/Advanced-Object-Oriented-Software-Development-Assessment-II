package ie.gmit.sw;

import java.util.ArrayList;
import java.util.Collection;

public class DeltaAddRemove {

	Collection<Lambda> betas = new ArrayList<>();
	Collection<Lambda> epsilons = new ArrayList<>();
	Collection<Lambda> zetas = new ArrayList<>();

	public void add(Lambda lambda) {
		betas.add(lambda);
		System.out.println("public void add(Lambda lambda) " + getClass() + "\n");
	}

	public void remove(Lambda lambda) {
		betas.remove(lambda);
		System.out.println("public void remove(Lambda lambda) " + getClass() + "\n");
	}

}
