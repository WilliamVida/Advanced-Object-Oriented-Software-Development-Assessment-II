package ie.gmit.sw;

public class Eta {

	public byte beta(byte b) {
		int i = b;
		return (byte) (i ^ 0xFFFFFFFF);
	}

	public int epsilon(int i) {
		return i & 0xFF000000;
	}

	public byte gamma(byte b) {
		return (byte) (Integer.reverse(b));
	}

	public int zeta(int i) {
		return i & 0x000000FF;
	}

}
