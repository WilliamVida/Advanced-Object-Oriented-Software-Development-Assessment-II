package ie.gmit.sw;

import java.util.Iterator;

/**
 * This class iterates through the collections of lambdas and gammas.
 * 
 * @author William Vida
 */

public class Omicron {

	Sigma sigma = new Sigma();
	
	/**
	 * Returns lambdas' in iterator.
	 */
	public Iterator<Lambda> lambdaIterator() {
		return sigma.lambdas.iterator();
	}

	/**
	 * Returns gammas in iterator.
	 */
	public Iterator<Gamma> gammaIterator() {
		return sigma.gammas.iterator();
	}

}
